class person:
    def __init__(self, name, surname, age=None, gender=None ):
        self.name = name
        self.surname = surname
        self.age = age
        self.gender = gender

    def walk(self):
        print('person', self.name, 'is waklking')

    def info(self):
        print('person name=',self.name, 'surname=',self.surname, 'age=',self.age, 'gender=' ,self.gender)
